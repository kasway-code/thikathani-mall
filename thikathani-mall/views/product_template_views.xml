<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="view.thikathani_product" model="ir.ui.view">
    <field name="active" eval="True"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//form[@name='Product Template']/sheet[1]/notebook[1]" position="inside">
          <page name="studio_page_boUGl" string="Mas Informacion">
            <group name="studio_group_boUGl">
              <field name="descripcion_origen" string="Descripcion y origen2"/>
              <field name="descripcion_beneficios" string="Beneficios2"/>
              <field name="descripcion_usos" string="Modos de uso2"/>
            </group>
          </page>
        </xpath>
      </data>
    </field>
    <field name="field_parent" eval="False"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="inherit_id" ref="product.product_template_only_form_view"/>
    <field name="key" eval="False"/>
    <field name="mode">extension</field>
    <field name="model">product.template</field>
    <field name="name">Odoo Studio: product.template.product.form customization</field>
    <field name="priority">99</field>
    <field name="type">form</field>
  </record>
</odoo>